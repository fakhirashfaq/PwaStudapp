{
  "name": "stubud-pwa",
  "version": "1.0.0",
  "description": "PWA for Stubud ltd",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "fireStart": "start firefox http://localhost:4200 && ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-package": "ng build --configuration production --common-chunk --delete-output-path --build-optimizer --output-hashing=all",
    "startOnIp": "ng serve -o --host 192.168.100.37",
    "build:stats": "ng build --configuration production --common-chunk --delete-output-path --build-optimizer --namedChunks=true --stats-json",
    "build:source-maps": "ng build --configuration production --common-chunk --delete-output-path --build-optimizer --namedChunks=true --source-map",
    "analyze": "webpack-bundle-analyzer dist/PwaStuBud/stats.json",
    "start-pwa": "ng build --configuration production && http-server -p 8080 -c-1 dist/angular-pwa",
    "firebaseIndex": "firebase deploy --only firestore:indexes",
    "getFireStoreIndexes": "firebase firestore:indexes",
    "functions": "firebase deploy --only firestore"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "^12.2.4",
    "@angular/cdk": "^12.2.4",
    "@angular/common": "^12.2.4",
    "@angular/compiler": "^12.2.4",
    "@angular/core": "^12.2.4",
    "@angular/fire": "^7.0.3",
    "@angular/forms": "^12.2.4",
    "@angular/localize": "^11.2.11",
    "@angular/platform-browser": "^12.2.4",
    "@angular/platform-browser-dynamic": "^12.2.4",
    "@angular/pwa": "^12.2.4",
    "@angular/router": "^12.2.4",
    "@angular/service-worker": "^12.2.4",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@kinect-pro/ngp-carousel": "^2.1.1",
    "@ngx-loading-bar/core": "^5.1.2",
    "@ngx-loading-bar/http-client": "^5.1.2",
    "@ngx-loading-bar/router": "^5.1.2",
    "algoliasearch": "^4.10.5",
    "angular-instantsearch": "^4.0.0",
    "cookieconsent": "^3.1.1",
    "core-js": "^3.17.2",
    "cors": "^2.8.5",
    "firebase": "^9.0.1",
