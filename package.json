    "npm-check-updates": "^11.8.4",
    "protractor": "^7.0.0",
    "rxjs-tslint": "^0.1.8",
    "source-map-explorer": "^2.5.2",
    "ts-node": "~10.2.1",
    "tslint": "^6.1.3",
    "typescript": "4.3.5",
    "webpack": "^5.51.2",
